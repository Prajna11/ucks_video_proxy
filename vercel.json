{
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/proxy"
    },
    {
      "source": "/proxy",
      "destination": "/api/proxy"
    }
  ],
  "headers": [
    {
      "source": "/api/proxy",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, HEAD, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Range, If-Range, If-Modified-Since, If-None-Match, Content-Type"
        }
      ]
    },
    {
      "source": "/proxy",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, HEAD, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Range, If-Range, If-Modified-Since, If-None-Match, Content-Type"
        }
      ]
    }
  ]
}